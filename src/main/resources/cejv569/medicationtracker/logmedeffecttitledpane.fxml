<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>
<TitledPane fx:id="logEffectTitledPane"
            animated="false" layoutX="10.0" layoutY="94.0"
            text="Log Medication Effect"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="cejv569.medicationtracker.view.viewcontrollers.LogMedicationEffectController">
<content>
    <ScrollPane prefHeight="700.0" prefWidth="1000.0">
        <content>
            <AnchorPane fx:id="logEffectAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="1000.0" style="-fx-background-color: #28d0ff;">
                <children>
                    <ImageView fitHeight="189.0" fitWidth="261.0" layoutX="36.0" layoutY="56.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@assets/effectshistory.jpg" />
                        </image>
                    </ImageView>
                    <VBox fillWidth="false" layoutX="332.0" layoutY="5.0" prefHeight="260.0" prefWidth="649.0" spacing="2.0">
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                        <children>
                            <HBox prefHeight="110.0" prefWidth="620.0">
                                <children>
                                    <TableView fx:id="EffectTableView" fixedCellSize="255.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="110.0" prefWidth="636.0" HBox.hgrow="NEVER">
                                        <columns>
                                            <TableColumn fx:id="purchaseDateTableColumn" editable="false" maxWidth="355.0" minWidth="82.0" prefWidth="177.0" sortType="DESCENDING" text="Medication Purchase Date" />
                                            <TableColumn fx:id="brandTableColumn" editable="false" maxWidth="273.0" minWidth="0.0" prefWidth="178.0" text="Medication Brand Name" />
                                            <TableColumn fx:id="genericTableColumn" editable="false" maxWidth="255.0" minWidth="255.0" prefWidth="255.0" text="Medication Generic Name" />
                                        </columns>
                                        <columnResizePolicy>
                                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                        </columnResizePolicy>
                                        <cursor>
                                            <Cursor fx:constant="HAND" />
                                        </cursor>
                                        <padding>
                                            <Insets left="5.0" right="5.0" />
                                        </padding>
                                    </TableView>
                                </children>
                            </HBox>
                            <HBox prefHeight="22.0" prefWidth="620.0" spacing="300.0">
                                <children>
                                    <Label style="-fx-font-family: Garamond; -fx-font-size: 14; -fx-font-weight: Bold;" text="Effect">
                                        <HBox.margin>
                                            <Insets />
                                        </HBox.margin>
                                    </Label>
                                    <Label layoutX="10.0" layoutY="10.0" style="-fx-font-family: Garamond; -fx-font-size: 14; -fx-font-weight: Bold;" text="Severity" />
                                </children>
                                <VBox.margin>
                                    <Insets top="10.0" />
                                </VBox.margin>
                            </HBox>
                            <HBox prefHeight="35.0" prefWidth="600.0" spacing="80.0">
                                <children>
                                    <ComboBox fx:id="selectEffectComboBox" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="255.0" promptText="Select Effect" style="-fx-font-family: Garamond; -fx-font-size: 14;" visibleRowCount="1">
                                        <cursor>
                                            <Cursor fx:constant="HAND" />
                                        </cursor>
                                        <padding>
                                            <Insets left="2.0" right="2.0" />
                                        </padding>
                                    </ComboBox>
                                    <ComboBox fx:id="selectSeverityComboBox" prefWidth="150.0" promptText="Select severity" style="-fx-font-family: Garamond; -fx-font-size: 14;" visibleRowCount="1">
                                        <cursor>
                                            <Cursor fx:constant="HAND" />
                                        </cursor></ComboBox>
                                </children>
                            </HBox>
                            <HBox prefHeight="20.0" prefWidth="620.0" spacing="243.0">
                                <children>
                                    <Label style="-fx-font-family: Garamond; -fx-font-size: 14; -fx-font-weight: Bold;" text="Effect Duration">
                                        <cursor>
                                            <Cursor fx:constant="TEXT" />
                                        </cursor>
                                        <HBox.margin>
                                            <Insets />
                                        </HBox.margin>
                                    </Label>
                                    <Label layoutX="10.0" layoutY="10.0" style="-fx-font-family: Garamond; -fx-font-size: 14; -fx-font-weight: Bold;" text="Date of effect">
                                        <cursor>
                                            <Cursor fx:constant="TEXT" />
                                        </cursor>
                                    </Label>
                                </children>
                                <VBox.margin>
                                    <Insets top="10.0" />
                                </VBox.margin>
                            </HBox>
                            <HBox prefHeight="31.0" prefWidth="620.0" spacing="83.0">
                                <children>
                                    <TextField fx:id="logDurationTextField" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="255.0" promptText="Enter how long the effect lasted" />
                                    <DatePicker fx:id="dateNoticedDatePicker" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="22.0" prefWidth="150.0">
                                        <cursor>
                                            <Cursor fx:constant="HAND" />
                                        </cursor>
                                    </DatePicker>
                                </children>
                            </HBox>
                            <HBox prefHeight="50.0" prefWidth="50.0" spacing="30.0">
                                <children>
                                    <Button fx:id="addEffectLogButton" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Add">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                    </Button>
                                    <Button fx:id="editEffectLogButton" contentDisplay="GRAPHIC_ONLY" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Edit">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                    </Button>
                                    <Button fx:id="saveEffectLogButton" contentDisplay="GRAPHIC_ONLY" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Save">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                    </Button>
                                    <Button fx:id="deleteEffectLogButton" contentDisplay="GRAPHIC_ONLY" layoutX="65.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Delete">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                    </Button>
                                </children>
                                <padding>
                                    <Insets left="5.0" top="15.0" />
                                </padding>
                                <VBox.margin>
                                    <Insets top="15.0" />
                                </VBox.margin>
                            </HBox>
                        </children>
                    </VBox>
                </children>
            </AnchorPane>
        </content>
    </ScrollPane>
</content>
</TitledPane>
