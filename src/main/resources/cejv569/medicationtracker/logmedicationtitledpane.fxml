<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<TitledPane fx:id="logMedicationTitledPane" animated="false"
            layoutX="10.0" layoutY="94.0" text="Log Medication Taken"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="cejv569.medicationtracker.view.viewcontrollers.LogMedicationController">
<content>
    <ScrollPane prefHeight="700.0" prefWidth="1000.0">
        <content>
            <AnchorPane fx:id="logMedicationAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="1000.0" style="-fx-background-color: #28d0ff;">
                <children>
                    <ImageView fitHeight="189.0" fitWidth="261.0" layoutX="36.0" layoutY="56.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@assets/usermedication.jpg" />
                        </image>
                    </ImageView>
                    <VBox fillWidth="false" layoutX="330.0" layoutY="15.0" prefHeight="215.0" prefWidth="673.0" spacing="10.0">
                        <children>
                            <HBox prefHeight="32.0" prefWidth="653.0" spacing="40.0">
                                <children>
                                    <Label prefWidth="300.0" text="Medication brand name">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                    <Label prefWidth="300.0" text="Medication generic name">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                </children>
                                <padding>
                                    <Insets bottom="2.0" top="2.0" />
                                </padding>
                            </HBox>
                            <HBox prefHeight="35.0" prefWidth="653.0" spacing="40.0">
                                <children>
                                    <ComboBox fx:id="brandMedTakenComboBox" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="300.0" promptText="Select Brand Name" style="-fx-font-family: Garamond; -fx-font-weight: Bold; -fx-font-size: 14;" />
                                    <ComboBox fx:id="genericMedTakenComboBox" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="300.0" promptText="Select Generic Name" style="-fx-font-family: Garamond; -fx-font-weight: Bold; -fx-font-size: 14;" />
                                </children>
                                <padding>
                                    <Insets bottom="2.0" top="2.0" />
                                </padding>
                            </HBox>
                            <HBox prefHeight="29.0" prefWidth="635.0" spacing="173.0">
                                <children>
                                    <Label prefHeight="17.0" prefWidth="169.0" text="Number of doses" wrapText="true">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                        <padding>
                                            <Insets right="20.0" />
                                        </padding>
                                    </Label>
                                    <Label prefHeight="17.0" prefWidth="115.0" text="Date &amp; time taken">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                </children>
                                <padding>
                                    <Insets bottom="2.0" right="10.0" top="2.0" />
                                </padding>
                            </HBox>
                            <HBox prefHeight="35.0" prefWidth="635.0" spacing="240.0">
                                <children>
                                    <TextField fx:id="dosesTakenTextField" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="100.0" />
                                    <DatePicker fx:id="dateTakenDatePicker" prefHeight="26.0" prefWidth="294.0" />
                                </children>
                                <padding>
                                    <Insets bottom="2.0" top="2.0" />
                                </padding>
                            </HBox>
                            <VBox prefHeight="59.0" prefWidth="635.0" spacing="10.0">
                                <children>
                                    <Label alignment="CENTER" prefHeight="18.0" prefWidth="102.0" text="Last Purchased">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                        <VBox.margin>
                                            <Insets left="338.0" />
                                        </VBox.margin>
                                    </Label>
                                    <Label fx:id="purchaseDateLabel" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="18.0" prefWidth="200.0" style="-fx-text-fill: BLACK; -fx-background-color: WHITE; -fx-font-family: Garamond; -fx-font-size: 14;" text="Purchased date">
                                        <cursor>
                                            <Cursor fx:constant="NONE" />
                                        </cursor>
                                        <VBox.margin>
                                            <Insets left="342.0" />
                                        </VBox.margin>
                                    </Label>
                                </children>
                                <padding>
                                    <Insets bottom="10.0" top="10.0" />
                                </padding>
                            </VBox>
                            <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="265.0" spacing="10.0">
                                <children>
                                    <Button fx:id="addMedTakenButton" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" prefHeight="120.0" prefWidth="60.0" text="Add">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                        <HBox.margin>
                                            <Insets />
                                        </HBox.margin>
                                    </Button>
                                    <Button fx:id="editMedTakenButton" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" prefHeight="60.0" prefWidth="50.0" text="Edit">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                    </Button>
                                    <Button fx:id="saveMedTakenButton" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" prefHeight="60.0" prefWidth="50.0" text="Save">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                    </Button>
                                    <Button fx:id="deleteMedTakenButton" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" prefHeight="60.0" prefWidth="50.0" text="Delete">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                    </Button>
                                </children>
                            </HBox>
                        </children>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" />
                        </padding>
                    </VBox>
                </children>
            </AnchorPane>
        </content>
    </ScrollPane>
</content>
</TitledPane>
