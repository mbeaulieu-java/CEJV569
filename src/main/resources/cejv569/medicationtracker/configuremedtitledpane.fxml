                            <HBox prefHeight="32.0" prefWidth="620.0" spacing="30.0">
                                <children>
                                    <Label prefWidth="300.0" text="Brand Name">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                    <Label prefWidth="300.0" text="Generic Name">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                </children>
                                <padding>
                                    <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                                </padding>
                            </HBox>
                            <HBox prefHeight="62.0" prefWidth="686.0" spacing="15.0">
                                <children>
                                    <ComboBox fx:id="brandComboBox" editable="true" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="300.0" promptText="Enter Brand Name" style="-fx-font-family: Garamond; -fx-font-weight: Bold; -fx-font-size: 14;" />
                                    <ComboBox fx:id="genericComboBox" editable="true" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="300.0" promptText="Enter Generic Name" style="-fx-font-family: Garamond; -fx-font-weight: Bold; -fx-font-size: 14;" />
