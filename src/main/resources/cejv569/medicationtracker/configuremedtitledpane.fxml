                                          <ImageView fitHeight="25.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@assets/addarrow.png" />
                                             </image>
                                          </ImageView>
                                       </graphic>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin></Button>
                                            <Button fx:id="removeIngredientButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" style="-fx-font-size: 25;" text="-">
                                       <graphic>
                                          <ImageView fitHeight="25.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@assets/removearrow.png" />
                                             </image>
                                          </ImageView>
                                       </graphic></Button>
                                        </children>
                                    </VBox>
                                    <ListView fx:id="ingredientsListView" fixedCellSize="300.0" layoutX="20.0" layoutY="12.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="72.0" prefWidth="203.0">
                                        <cursor>
                                            <Cursor fx:constant="TEXT" />
                                        </cursor>
                                 <padding>
                                    <Insets left="10.0" right="10.0" top="30.0" />
                                 </padding>
                                    </ListView>
                                </children>
                            </HBox>
                            <HBox alignment="TOP_CENTER" layoutX="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="290.0" spacing="17.0">
                                <children>
                                    <Button fx:id="addMedicationButton" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="50.0" prefWidth="60.0" text="Add">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                 <graphic>
                                    <ImageView fitHeight="30.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@assets/add.png" />
                                       </image>
                                    </ImageView>
                                 </graphic>
                                    </Button>
                              <Button alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="50.0" prefWidth="60.0">
                                 <graphic>
                                    <ImageView fitHeight="30.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@assets/edit.png" />
                                       </image>
                                    </ImageView>
                                 </graphic>
                              </Button>
                                    <Button fx:id="saveMedicationButton" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" layoutX="10.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="50.0" prefWidth="60.0">
                                        <cursor>
                                            <Cursor fx:constant="OPEN_HAND" />
                                        </cursor>
                                 <graphic>
                                    <ImageView fitHeight="30.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@assets/save.png" />
                                       </image>
                                    </ImageView>
                                 </graphic>
                                    </Button>
                                </children>
                                <VBox.margin>
                                    <Insets left="13.0" />
                                </VBox.margin>
                            </HBox>
                        </children>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                    </VBox>
                </children></AnchorPane>
        </content>
    </ScrollPane>
</content>
</TitledPane>
