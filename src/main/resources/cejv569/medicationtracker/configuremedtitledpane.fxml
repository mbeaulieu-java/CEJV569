                                </children>
                            </HBox>
                            <HBox prefHeight="21.0" prefWidth="643.0" spacing="207.0">
                                <children>
                                    <Label prefWidth="300.0" text="Medication Ingredients">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                    <Label layoutX="20.0" layoutY="12.0" prefWidth="300.0" text="Available Ingredients">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                </children>
                                <padding>
                                    <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                                </padding>
                            </HBox>
                            <HBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="100.0" prefWidth="300.0" spacing="25.0">
                                <padding>
                                    <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                                </padding>
                                <children>
                                    <ListView fx:id="medIngredientsListView" editable="true" fixedCellSize="300.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="73.0" prefWidth="300.0" HBox.hgrow="NEVER">
                                        <padding>
                                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                        </padding>
                                        <cursor>
                                            <Cursor fx:constant="TEXT" />
                                        </cursor>
                                    </ListView>
                                    <VBox prefHeight="80.0" prefWidth="54.0" spacing="10.0">
                                        <children>
                                            <Button fx:id="addIngredientButton" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" style="-fx-font-size: 16; -fx-text-fill: BLACK;">
