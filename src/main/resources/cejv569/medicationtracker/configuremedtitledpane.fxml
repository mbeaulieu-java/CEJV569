                                <padding>
                                    <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                                </padding>
                            </HBox>
                            <HBox prefHeight="40.0" prefWidth="620.0" spacing="38.0">
                                <children>
                                    <Label prefWidth="300.0" text="Format">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                    <Label prefWidth="300.0" text="Measurement Unit">
                                        <font>
                                            <Font name="Garamond Bold" size="14.0" />
                                        </font>
                                    </Label>
                                </children>
                                <padding>
                                    <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                                </padding>
                            </HBox>
                            <HBox prefHeight="62.0" prefWidth="686.0" spacing="15.0">
                                <padding>
                                    <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                                </padding>
                                <children>
                                    <ComboBox fx:id="formatComboBox" editable="true" prefWidth="300.0" promptText="Enter or select format" />
